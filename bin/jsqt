#!/bin/bash
#
# A wrapper for jsq test runner
#

CUR="${BASH_SOURCE[0]}";
if([ -h "${CUR}" ]) then
  while([ -h "${CUR}" ]) do CUR=`readlink "${CUR}"`; done
fi
pushd . > /dev/null
cd `dirname ${CUR}` > /dev/null
CUR=`pwd`;
popd  > /dev/null

exec "node" "${CUR}/org/jsq/tools/TestRunner.js" "${CUR}/.." "$@"
